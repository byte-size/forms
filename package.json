{
  "name": "bs-forms",
  "main": "./dist/umd.js",
  "module": "./dist/esm.js",
  "browser": "./dist/min.js",
  "files": [
    "dist/*"
  ],
  "version": "1.0.0",
  "description": "Focus trapping and external validation schema configurable Tailwind UI compatible form functionality components",
  "scripts": {
    "dev": "nodemon -e js,vue --watch src --exec npm run build",
    "build": "npm run build:umd & npm run build:es & npm run build:iife",
    "build:es": "rollup --config rollup.config.js --format es --file dist/esm.js",
    "build:umd": "rollup --config rollup.config.js --format umd --file dist/umd.js",
    "build:iife": "rollup --config rollup.config.js --format iife --file dist/min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/byte-size/forms.git"
  },
  "keywords": [
    "Tailwind",
    "Tailwind UI",
    "Vue JS",
    "Forms",
    "Validation",
    "Focus Trap"
  ],
  "author": "Kemal Yalcinkaya",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/byte-size/forms/issues"
  },
  "homepage": "https://github.com/byte-size/forms#readme",
  "dependencies": {
    "focus-trap": "^5.1.0",
    "form-serialize": "^0.7.2",
    "superstruct": "^0.8.3",
    "v-click-outside": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@babel/preset-env": "^7.8.6",
    "nodemon": "^2.0.2",
    "rollup": "^1.32.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-filesize": "^6.2.1",
    "rollup-plugin-internal": "^1.0.4",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^2.1.1",
    "rollup-plugin-terser": "^5.2.0",
    "rollup-plugin-vue": "^5.1.6",
    "vue": "^2.6.11",
    "vue-template-compiler": "^2.6.11"
  }
}
