{
  "name": "bs-forms",
  "main": "./dist/umd.js",
  "module": "./dist/esm.js",
  "files": [
    "dist/*"
  ],
  "version": "0.1.1",
  "description": "Focus trapping and external validation schema configurable Tailwind UI compatible form functionality components",
  "scripts": {
    "dev": "nodemon -e js,vue --watch src --exec npm run build",
    "build": "npm run build:umd & npm run build:es",
    "build:es": "rollup --config rollup.config.js --format es --file dist/esm.js",
    "build:umd": "rollup --config rollup.config.js --format umd --file dist/umd.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/byte-size/forms.git"
  },
  "keywords": [
    "Tailwind",
    "Tailwind UI",
    "Vue JS",
    "Forms",
    "Validation",
    "Focus Trap"
  ],
  "author": "Kemal Yalcinkaya",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/byte-size/forms/issues"
  },
  "homepage": "https://github.com/byte-size/forms#readme",
  "dependencies": {
    "focus-trap": "^6.0.1",
    "form-serialize": "^0.7.2",
    "v-click-outside": "^3.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "nodemon": "^2.0.4",
    "rollup": "^2.27.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-filesize": "^9.0.2",
    "rollup-plugin-internal": "^1.0.4",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^5.1.9",
    "vue": "^2.6.12",
    "vue-template-compiler": "^2.6.12"
  }
}
